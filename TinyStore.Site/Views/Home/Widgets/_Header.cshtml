<!--文档编码格式-->
<meta charset="utf-8">
<!--让 IE 浏览器运行最新的渲染模式下-->
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<!--多内核浏览器默认采用webkit高速模式渲染页面-->
<meta name="renderer" content="webkit">
<!--页面显示宽度,默认为设备宽度,默认缩放比例为1-->
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1,maximum-scale =1,user-scalable=no">
<meta name="apple-mobile-web-app-capable" content="yes" />
<meta name="full-screen" content="true" />
<meta name="x5-fullscreen" content="true" />
<meta name="360-fullscreen" content="true" />

<meta http-equiv="Cache-Control" content="no-transform" />
<meta http-equiv="Cache-Control" content="no-siteapp">

<script src="//cdn.staticfile.org/jquery/3.5.1/jquery.min.js"
        asp-fallback-test="window.jQuery"
        asp-fallback-src="~/lib/jquery/dist/jquery.min.js"></script>

<script src="//cdn.staticfile.org/dot/1.1.3/doT.min.js"
        asp-fallback-test="window.doT"
        asp-fallback-src="~/lib/dot/doT.min.js"></script>

<link rel="stylesheet" type="text/css" href="//at.alicdn.com/t/font_1964411_nboy3sdd1c.css"
      asp-fallback-test-class="iconfont"
      asp-fallback-test-property="font-size"
      asp-fallback-test-value="16px"
      asp-fallback-href="~/iconfonts/iconfont.css">

<script crossorigin="anonymous" 
    src="//lib.baomitu.com/jBox/1.3.3/jBox.all.min.js"
    asp-fallback-test="window.jBox"
    asp-fallback-src="~/lib/jbox/dist/jBox.all.min.js"></script>

<link crossorigin="anonymous" rel="stylesheet"
    href="//lib.baomitu.com/jBox/1.3.3/jBox.all.min.css"
    asp-fallback-test-class="jBox-wrapper"
    asp-fallback-test-property="text-align"
    asp-fallback-test-value="left"
    asp-fallback-href="~/lib/jbox/dist/jBox.all.min.css">

<script src="//cdn.staticfile.org/popper.js/2.11.5/umd/popper.min.js"
        asp-fallback-test="window.Popper"
        asp-fallback-src="~/lib/popper.js/umd/popper.min.js"></script>

<script src="//cdn.staticfile.org/tippy.js/6.3.7/tippy.umd.min.js"
        asp-fallback-test="window.tippy"
        asp-fallback-src="~/lib/tippy.js/dist/tippy.umd.min.js"></script>

<link rel="stylesheet" type="text/css" href="//cdn.staticfile.org/tippy.js/6.3.7/tippy.min.css"
      asp-fallback-test-class="tippy-box"
      asp-fallback-test-property="line-height"
      asp-fallback-test-value="1.4"
      asp-fallback-href="~/lib/tippy.js/dist/tippy.min.css">

<environment include="Development">
    <link href="~/css/default.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Scripts/jquery.CY.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.QQService.js" type="text/javascript" asp-append-version="true"></script>
</environment>
<environment exclude="Development">
    <link href="~/css/default.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Scripts/jquery.CY.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.QQService.js" type="text/javascript" asp-append-version="true"></script>
</environment>

<partial name="Widgets/_Plugin_Toast" />

<script>
    $("html").css("font-size", window.innerWidth < 540 ? window.innerWidth / 10 + "px" : "54px");

    CY.AjaxUrl = "/api";
    CY.MVC = true;
    CY.AjaxMask = false;
</script>