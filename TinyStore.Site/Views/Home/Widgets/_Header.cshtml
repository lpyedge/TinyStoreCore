<environment include="Development">
    <link href="~/css/default.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Scripts/jquery.CY.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.QQService.js" type="text/javascript" asp-append-version="true"></script>
</environment>
<environment exclude="Development">
    <link href="~/css/default.css" rel="stylesheet" asp-append-version="true" />
    <script src="~/Scripts/jquery.CY.js" type="text/javascript" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.QQService.js" type="text/javascript" asp-append-version="true"></script>
</environment>

<partial name="Widgets/_Plugin_Toast" />

<script>
    $("html").css("font-size", window.innerWidth < 540 ? window.innerWidth / 10 + "px" : "54px");

    CY.AjaxUrl = "/api";
    CY.MVC = true;
    CY.AjaxMask = false;
</script>