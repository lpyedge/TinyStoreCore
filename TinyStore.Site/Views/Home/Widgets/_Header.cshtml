<!--文档编码格式-->
<meta charset="utf-8">
<!--让 IE 浏览器运行最新的渲染模式下-->
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<!--多内核浏览器默认采用webkit高速模式渲染页面-->
<meta name="renderer" content="webkit">
<!--页面显示宽度,默认为设备宽度,默认缩放比例为1-->
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1,maximum-scale =1,user-scalable=no">
<meta name="apple-mobile-web-app-capable" content="yes" />
<meta name="full-screen" content="true" />
<meta name="x5-fullscreen" content="true" />
<meta name="360-fullscreen" content="true" />

<meta http-equiv="Cache-Control" content="no-transform" />
<meta http-equiv="Cache-Control" content="no-siteapp">

<script src="//cdn.staticfile.org/jquery/3.5.1/jquery.min.js"
        asp-fallback-test="window.jQuery"
        asp-fallback-src="~/lib/jquery/dist/jquery.min.js"></script>

<script src="//cdn.staticfile.org/dot/1.1.3/doT.min.js"
        asp-fallback-test="window.doT"
        asp-fallback-src="~/lib/dot/doT.min.js"></script>
        
<link href="https://cdn.staticfile.org/font-awesome/5.15.4/css/all.min.css" rel="stylesheet" type="text/css"
      asp-fallback-test-class="fa-pull-left"
      asp-fallback-test-property="float"
      asp-fallback-test-value="left"
      asp-fallback-href="https://lib.baomitu.com/font-awesome/5.15.4/css/all.min.css">

<script crossorigin="anonymous" 
    src="//lib.baomitu.com/jBox/1.3.3/jBox.all.min.js"
    asp-fallback-test="window.jBox"
    asp-fallback-src="~/lib/jbox/dist/jBox.all.min.js"></script>

<link crossorigin="anonymous" rel="stylesheet"
    href="//lib.baomitu.com/jBox/1.3.3/jBox.all.min.css"
    asp-fallback-test-class="jBox-wrapper"
    asp-fallback-test-property="text-align"
    asp-fallback-test-value="left"
    asp-fallback-href="~/lib/jbox/dist/jBox.all.min.css">

